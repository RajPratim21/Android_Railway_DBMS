<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raj.railwaydbms2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Passenger"
            android:label="@string/title_activity_passenger"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Train"
            android:label="@string/title_activity_train"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Tracks"
            android:label="@string/title_activity_tracks"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TrainSchedule"
            android:label="@string/title_activity_train_schedule"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Station"
            android:label="@string/title_activity_station"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Display_passenger" />
        <activity android:name=".Display_track" />
        <activity android:name=".Display_train" />
        <activity android:name=".Display_schedule"></activity>
    </application>

</manifest>